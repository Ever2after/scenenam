  var menu = 0;
  $(document).ready(function(){
    $('#menuimg').click(function(){
      if(menu==0){
        $('.menu').show();
        $('.menu').addClass('slideRight');
        menu=1;
      }else {
        $('.menu').hide();
        menu=0;
      }
    });
  });
